######################################################################
# some env settings
######################################################################

# use vi style in shell
set -o vi

# dircolors.256dark
eval `dircolors ~/.dircolors`

# xterm settings
xrdb ~/.Xresources

# aliases
alias sl='ls'
alias vi='vim'
alias start_venv='source venv/bin/activate'
 
# env settings
export PATH=$PATH:$HOME/sbin
export TERM="xterm-256color"
